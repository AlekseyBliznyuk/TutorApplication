{
  "_version": "1.12.0",
  "sap.app": {
    "id": "student.lessons",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "Student Lessons App"
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.97.0",
      "libs": {
        "sap.m": {},
        "sap.fe.core": {},
        "sap.ui.core": {},
        "sap.ui.table": {}
      }
    },
    "models": {
      "": {
        "dataSource": "CatalogService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "earlyRequests": true,
          "annotations": [
            "webapp/annotations/annotations.xml"
          ]
        },
        "type": "sap.ui.model.odata.v4.ODataModel"
      }
    },
    "routing": {
      "routes": [
        { "pattern": "", "name": "ListReport", "target": "ListReport" },
        { "pattern": "Students/{ID}", "name": "ObjectPage", "target": "ObjectPage" }
      ],
      "targets": {
        "ListReport": {
          "type": "Component",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Students",
              "navigation": { "to_ObjectPage": { "detail": "ObjectPage" } }
            }
          }
        },
        "ObjectPage": {
          "type": "Component",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": { "entitySet": "Students" }
          }
        }
      }
    }
  },
  "sap.ui": { "technology": "UI5" },
  "sap.cloud": { "service": "html5" }
}
